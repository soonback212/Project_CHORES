{
  "configurations": [
    {
      "name": "STM32",
      "includePath": [
        "${workspaceFolder}/../USB_HOST/App",
        "${workspaceFolder}/../USB_HOST/Target",
        "${workspaceFolder}/../Core/Inc",
        "${workspaceFolder}/../Drivers/STM32F4xx_HAL_Driver/Inc",
        "${workspaceFolder}/../Drivers/STM32F4xx_HAL_Driver/Inc/Legacy",
        "${workspaceFolder}/../Middlewares/ST/STM32_USB_Host_Library/Core/Inc",
        "${workspaceFolder}/../Middlewares/ST/STM32_USB_Host_Library/Class/CDC/Inc",
        "${workspaceFolder}/../Drivers/CMSIS/Device/ST/STM32F4xx/Include",
        "${workspaceFolder}/../Drivers/CMSIS/Include",
        "/opt/ros/humble/include/**",
        "/usr/include/**",
        "/usr/include/c++/11",
        "/usr/include/x86_64-linux-gnu/**",
        "/usr/local/include/**",
        "/home/your_user/ros2_ws/install/**"
      ],
      "defines": ["DEBUG", "USE_HAL_DRIVER", "STM32F407xx"],
      "compilerPath": "/opt/homebrew/bin/arm-none-eabi-gcc",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "intelliSenseMode": "gcc-arm"
    },
    {
      "name": "ROS2",
      "includePath": [
        "/opt/ros/humble/include",
        "/opt/ros/humble/include/**",
        "/usr/include",
        "/usr/include/**",
        "/usr/local/include",
        "${workspaceFolder}/**"
      ],
      "defines": [],
      "compilerPath": "/usr/bin/g++",
      "cStandard": "c17",
      "cppStandard": "c++17",
      "intelliSenseMode": "linux-gcc-x64",
      "compileCommands": "${workspaceFolder}/build/compile_commands.json"
    }
  ],
  "version": 4
}
